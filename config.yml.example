# Copy this file to config.yml and configure as appropriate.
#
#

# Root directory of application
root: /path/to/system-check

# Commands to be executed
# Use quotes if commands have arguments
commands:
  - uptime
  - 'free -m'
  - w
  - ps
  - 'df -h'

# Contact list; use fully-qualified email addresses
contacts:
  - email@domain.ext

